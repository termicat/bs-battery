{
  "name": "Nextjs Template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "pnpm run --filter \"apps-*\" build",
    "build:me": "pnpm run --filter \"apps-*\" build:me",
    "build:battery": "pnpm run --filter \"apps-battery\" build && mv apps/battery/dist ./dist",
    "build:radar": "pnpm run --filter \"apps-radar\" build && mv apps/radar/dist ./dist"
  },
  "dependencies": {},
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "20.4.0",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^13.5.6",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "output": "dist"
}
